Algoritmo boolean
	a <- 5 == 3    // a = Falso, ya que 5 no es igual a 3
	b <- 4 <> 3   // b = Verdadero, ya que 4 es diferente a 3
	c <- 7 > 7    // c = Falso, ya que 7 no es mayor que 7, es igual a 7
	d <- 4 < 4    // d = Falso, ya que 4 no es menor que 4, es igual a 4
	e <- 100 <= 90   // e = Falso, ya que 100 no es menor o igual a 90
	f <- 40 >= 40   // f = Verdadero, ya que 40 es mayor o igual a 40
FinAlgoritmo
